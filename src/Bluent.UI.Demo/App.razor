@using Bluent.UI.Demo.Pages.Pages
<Router NotFoundPage="typeof(NotFound)" 
        AppAssembly="@typeof(App).Assembly" 
        AdditionalAssemblies="new []{ typeof(Bluent.UI.Demo.Pages._Imports).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector=".focus" />
    </Found>
</Router>
